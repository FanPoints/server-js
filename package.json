{
    "name": "@fanpoints/server-js",
    "version": "0.1.0",
    "type": "module",
    "description": "The SDK that allows to integrate FanPoints on the server-side.",
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "types": "dist/types.d.ts",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint src --ext .js,.ts",
        "check-types": "tsc --noemit",
        "docs": "npx typedoc",
        "fetch-secrets": ". ./scripts/fetch-secrets.sh",
        "fetch-secrets-test": ". ./scripts/fetch-secrets.sh stg",
        "build": "rm -rf build && rollup -c",
        "publish": "npm run build && npm publish --access public",
        "generate-graphql": "doppler run --mount ./scripts/backendRepoCredentials.json --mount-template ./doppler/backendRepoCredentials.tmpl.json -- graphql-codegen --config ./scripts/codegen.ts"
    },
    "author": "FanPoints",
    "license": "MIT",
    "homepage": "https://fanpoints.github.io/server-js/",
    "devDependencies": {
        "@graphql-codegen/cli": "5.0.0",
        "@graphql-codegen/client-preset": "^4.1.0",
        "@graphql-codegen/typescript": "4.0.1",
        "@graphql-codegen/typescript-graphql-request": "^6.0.1",
        "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-typescript": "^11.1.5",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "eslint-plugin-unused-imports": "^3.0.0",
        "graphql": "^16.8.1",
        "husky": "^8.0.3",
        "prettier": "^3.1.0",
        "rollup": "^4.6.0",
        "rollup-plugin-dts": "^6.1.0",
        "sync-request": "^6.1.0",
        "typedoc": "^0.25.4",
        "typedoc-material-theme": "^1.0.1",
        "typescript": "^5.2.2",
        "@rollup/plugin-node-resolve": "^15.2.3"
    },
    "dependencies": {
        "@aws-sdk/client-appsync": "^3.462.0",
        "graphql-request": "^6.1.0"
    }
}
